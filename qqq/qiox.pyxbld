def make_ext(modname, pyxfilename):
    from distutils.extension import Extension
    ext = Extension(name = modname,
                    sources=[pyxfilename],
                    extra_compile_args=['-march=native', '-O2', '-flto'],
                    extra_link_args=['-lflif'])
    return ext

def make_setup_args():
    return dict(script_args=["--verbose"])
